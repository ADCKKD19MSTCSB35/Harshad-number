import java.util.*;
class Demo
{
public static void main(String args[])
{
int m,n,r=0,sum=0;
Scanner t=new Scanner(System.in);
m=t.nextInt();
n=m;
while(m>0)
{
	r=m%10;
	sum+=r;
	m/=10;
}
if(n%sum==0)
{
	System.out.println("Harshad");
	}
	else
{
	System.out.println("not Harshad");
}
}
}
Input:
156
Output:
Harshad
